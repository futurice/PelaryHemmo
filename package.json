{
  "name": "Hemmo",
  "author": "Futurice",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "homepage": "http://getpepperoni.com",
  "bugs": "https://github.com/futurice/PelaryHemmo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/futurice/PelaryHemmo"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:android": "./bundle_android.sh",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/Hemmo/main.jsbundle --dev=false --minify --verbose",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "mocha src/**/*.spec.js",
    "test:watch": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'npm test'",
    "coverage": "rimraf coverage && istanbul cover --i=src/**/*.js _mocha -- src/**/*.spec.js",
    "check-coverage": "istanbul check-coverage"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "es6-symbol": "^3.1.1",
    "event-emitter": "^0.3.5",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-alpha.12",
    "react-dom": "15.6.1",
    "react-mixin": "^3.0.5",
    "react-native": "^0.45.1",
    "react-native-audio-toolkit": "1.0.5",
    "react-native-form": "^2.1.2",
    "react-native-gesture-password": "^0.2.0",
    "react-native-image-picker": "^0.26.3",
    "react-native-lock": "futurice/react-native-lock#feature/customizedTheme",
    "react-native-orientation": "git+https://github.com/yamill/react-native-orientation.git",
    "react-native-passcode-auth": "^1.0.0",
    "react-native-popup-menu": "^0.7.5",
    "react-native-progress-bar": "^0.1.2",
    "react-native-progress-bar-classic": "^1.2.1",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-loop": "^2.2.2",
    "redux-loop-symbol-ponyfill": "^2.2.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "standard-http-error": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-native": "^2.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "chokidar-cli": "^1.2.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.4.2",
    "react-addons-test-utils": "15.6.0",
    "react-native-mock": "^0.3.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.5"
  }
}
